{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "dev": "vite --host",
    "dev:all": "npm run dev:server:start & npm run dev",
    "dev:server:start": "tsx standalone-server.js",
    "dev:server:stop": "pkill -f 'tsx standalone-server.js' || true",
    "dev:server-no-watch": "NODE_ENV=test tsx standalone-server.js",
    "build": "npm install && vite build && mkdir -p api && echo \"import app from '../server/index.js'; export const handler = app;\" > api/index.js",
    "postbuild": "ls -R dist",
    "start": "NODE_ENV=production node dist/index.js",
    "lint": "echo \"Linter is currently disabled\"",
    "test": "RAPIDAPI_KEY=5706440c2cmsh6fbbaa0ddac2a64p116bd6jsn30a4d4fc5261 NODE_ENV=test node run-tests.js",
    "test:e2e": "playwright test",
    "type-check": "tsc --noEmit",
    "check": "npm run type-check && npm run lint",
    "db:push": "drizzle-kit push"
  },
  "nodemonConfig": {
    "watch": [
      "server/"
    ],
    "ext": "ts,json",
    "exec": "tsx standalone-server.js",
    "signal": "SIGTERM",
    "delay": "2500"
  },
